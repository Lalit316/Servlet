<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>user</servlet-name>
        <servlet-class>controller.UserServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>quiz</servlet-name>
        <servlet-class>controller.QuestionServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>quiz</servlet-name>
        <url-pattern>/question</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>quiz</servlet-name>
        <url-pattern>/quiz</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>user</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>quiz</servlet-name>
        <url-pattern>/questionForm</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>quiz</servlet-name>
        <url-pattern>/addQuestion</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>quiz</servlet-name>
        <url-pattern>/deletequestion</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>quiz</servlet-name>
        <url-pattern>/editquestion</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>quiz</servlet-name>
        <url-pattern>/edit1</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>user</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>user</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>user</servlet-name>
        <url-pattern>/userList</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>user</servlet-name>
        <url-pattern>/userForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>user</servlet-name>
        <url-pattern>/addUser</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>user</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>user</servlet-name>
        <url-pattern>/editUser</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>user</servlet-name>
        <url-pattern>/edit</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>user</servlet-name>
        <url-pattern>/userCompany</url-pattern>
    </servlet-mapping>









</web-app>
